{
    "rules": {
        "comma-dangle": [2, "never"],
        "no-console": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [2, "functions"],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-unreachable": 2,
        "no-unused-vars": 2,
        "valid-jsdoc": 2,
        "valid-typeof": 2,
        "block-scoped-var": 2,
        "curly": 2,
        "eqeqeq": [2, "smart"],
        "keyword-spacing": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-implied-eval": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-redeclare": [2, { "builtinGlobals": true }],
        "no-script-url": 2,
        "no-sequences": 2,
        "no-unused-expressions": [2, { "allowShortCircuit": true }],
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-with": 2,
        "yoda": 2,
        "semi": [2, "never"],
        "no-catch-shadow": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-undef-init": 2,
        "no-undef": 2,
        "global-require": 2,
        "no-new-require": 2,
        "array-bracket-spacing": [2, "never"],
        "block-spacing": [2, "always"],
        "brace-style": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "computed-property-spacing": [2, "never"],
        "eol-last": 2,
        "indent": 2,
        "max-depth": [2, 3],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 2,
        "no-multiple-empty-lines": [2, {"max": 1}],
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": 2,
        "operator-assignment": [2, "always"],
        "padded-blocks": [2, "never"],
        "quotes": [2, "single"],
        "space-before-blocks": 2,
        "space-before-function-paren": 2,
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": 2
    },
    "env": {
        "browser": true,
        "node": true
    }
}